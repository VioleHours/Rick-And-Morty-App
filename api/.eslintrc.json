{
    "env": {
        "node": true,
        "es60": true,
        "mocha": true
    },
    "extends": "airbnb/base",
    "settings": {
        "import/resolver": {
            "node": {
                "moduleDirectory": ["src", "node_modules"]
            }
        }
    },
    "rules": {
        "no-underscore-dangle": 0,
        "no-param-reassing": 0,
        "import/no-unresolved": 0
    }
}